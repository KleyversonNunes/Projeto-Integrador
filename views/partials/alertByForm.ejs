<% if(warning) { %>
    <% warning.forEach((erro) => { %>
        <div class="alert alert-danger alert-width alert-dismissible fade show div-alert p-1 mt-3" role="alert">
          <%= erro %>
          <button type="button" class="btn-close p-2" data-bs-dismiss="alert" aria-label="Close"></button>
        </div>
    <% }) %>
  <% } %>